{
    "expo": {
        "name": "potable",
        "slug": "map-app",
        "version": "1.0.12",
        "orientation": "portrait",
        "icon": "./assets/potable-low-poly-logo.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/potable-low-poly-logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#FEFEFE"
        },
        "jsEngine": "hermes",
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "infoPlist": {
                "UIBackgroundModes": ["location", "fetch"]
            },
            "bundleIdentifier": "com.potable.water",
            "buildNumber": "12"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon.png",
                "backgroundColor": "#D3D3D3"
            },
            "permissions": [
                "android.permission.CAMERA",
                "android.permission.RECORD_AUDIO"
            ],
            "package": "com.potable.water"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "eas": {
                "projectId": "c7bdc156-09fe-4c9f-92e8-4c8f359efebe"
            }
        },
        "plugins": [
            [
                "expo-camera",
                {
                    "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
                }
            ],
            "@react-native-google-signin/google-signin"
        ]
    }
}
